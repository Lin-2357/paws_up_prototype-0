.story{
  position: absolute;
  height: 18.5%;
  width: 25%;
  top: 23.3%;
  left: 57%;
  transform: rotate(-27.5deg);
}

.login{
  position: absolute;
  height: 18%;
  width: 18%;
  top: 47%;
  left: 74.5%;
}

.hov{
  background-color: rgba(50, 50, 50, 1);
  color: rgba(205, 205, 205, 1);
}
